{
  "name": "dayjs-financialdate",
  "version": "0.1.3",
  "description": "financial plugin for dayjs",
  "main": "./dist/financial-plugin.min.cjs",
  "module": "./dist/financial-plugin.min.js",
  "exports": {
    ".": {
        "import": "./dist/financial-plugin.min.js",
        "require": "./dist/financial-plugin.min.cjs"
    }
  },
  "scripts": {
    "test": "npx vitest",
    "build": "npx esbuild --minify --sourcemap=external --outfile=./dist/financial-plugin.min.cjs --target=es2015 --format=cjs src/FinancialPlugin.js && npx esbuild --minify --sourcemap=external --outfile=./dist/financial-plugin.min.js --target=es2015 --format=esm src/FinancialPlugin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankh-llc/dayjs-financial.git"
  },
  "keywords": [
    "dayjs",
    "financial",
    "quarter",
    "year",
    "date",
    "business"
  ],
  "author": "taiki katayama",
  "license": "MIT",
  "types": "./src/FinancialPlugin.d.ts",
  "bugs": {
    "url": "https://github.com/ankh-llc/dayjs-financial/issues"
  },
  "homepage": "https://github.com/ankh-llc/dayjs-financial#readme",
  "devDependencies": {
    "c8": "^7.11.0",
    "dayjs": "^1.11.0",
    "esbuild": "^0.14.31",
    "vitest": "^0.8.4"
  }
}
